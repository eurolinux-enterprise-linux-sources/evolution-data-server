<programlisting><link linkend="EAccount"><returnvalue>EAccount</returnvalue></link> *          e_account_new_from_xml              (<parameter>const <link linkend="gchar"><type>gchar</type></link> *xml</parameter>);</programlisting>
<variablelist role="params">
<varlistentry><term><parameter>xml</parameter>&#160;:</term>
<listitem><simpara>an XML account description
</simpara></listitem></varlistentry>
